{
    "Serilog": {
        "Using": [
            "Serilog.Sinks.File"
        ],
        "MinimumLevel": {
            "Default": "Warning",
            "Override": {
                "System": "Warning",
                "Microsoft": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "log.txt",
                    "rollingInterval": "Day"
                }
            }
        ],
        "Enrich": [
            "FromLogContext"
        ]
    },
    "ConnectionStrings": {
        "Main": "Host=localhost;Port=5432;Database=estimmo;Username=estimmo;Password=;"
    },
    "Jwt": {
        "Issuer": "https://estimmo-api.jmh.me",
        "Audience": "estimmo",
        "Key": "",
        "ExpirationInHours": 24
    }
}
